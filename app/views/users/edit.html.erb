<h1>編集</h1>
<%= form_with model: @user, url: users_update_path do |f| %>
    <label for="name">name</label>
    <%= f.text_field :name %>

    <label for="email">email</label>
    <%= f.text_field :email %>

    <label for="password">password</label>
    <%= f.text_field :password, type: "password" %>

    <%= f.submit :更新 %>
<% end %>